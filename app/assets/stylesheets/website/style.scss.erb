/* navbar section */

#main_menu{
  background-color: rgba(0, 0, 0, 0.2);
  transition: linear all 1s;
}

#main_menu .navbar-nav > li {
  top: 3px;
}

.menu-scroll{
  padding-top: 0;
  padding-bottom: 0;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 300;
  box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
  -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
  background-color: rgb(255, 255, 255) !important;
  -webkit-transition:all .3s;
  -moz-transition:all .3s;
  transition:all .3s;
}
.nave_menu{
  margin: 0;
  top: -8px;
  position: relative;
}

.nave_menu .collapse.in{
  transition: all 0s;
  background: #e7e7e7;

}

.nave_menu .collapse.in .navbar-nav > li > a {
  color: #6d6d6d;
}

.nave_menu .collapse.in .navbar-nav > li.active > a {
  color: #fa7c06;
}

.main_menu_bg{
  margin-top: 0;
}
.main_menu_bg > .container > .row{

}
.main_menu_bg .navbar-default {
  background-color: transparent;
  border-color: transparent;
  transition: all .6s;
  margin: 15px auto;
}
@media (min-width: 768px){
  .navbar-header {
    margin-top: 3px;
  }
}
.navbar-default .navbar-nav>li>a:hover{
  color:#fa7c06;
  background: transparent;
}
.main_menu_bg .navbar {
  min-height: 52px;
  position: relative;
  top: 8px;
  margin: 0;
  padding: 0;
}

.main_menu_bg .navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:hover{
  color: #fa7c06;
  padding-bottom: 8px;
  border-bottom: 1px solid #fa7c06;
  background: transparent;
}

.main_menu_bg .navbar-nav>li>a {
  padding-top: 15px;
  padding-bottom: 20px;
  text-transform: uppercase;
  font-size: 0.875rem;
  color: #bdbdbd;
  font-weight: bolder;
}
.menu-scroll > .main_menu_bg .navbar-nav>li>a {
  color: #6d6d6d;
}
.navbar-brand {
  padding: 0 15px;
}
.navbar-brand>img {
  max-height: 80%;
  top: 8%;
  position: relative;
}
.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:hover, .navbar-default .navbar-nav>.open>a:focus{
  background-color:transparent;
  color:#f5c239;

}

