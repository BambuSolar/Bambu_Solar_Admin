/*
Separator
*/
.separator {
  width: 50px;
  height: 50px;
  margin: 10px auto;
  background: none;
  position: relative;
  display: inline-block;
  border-radius: 50%;
}
.separator:after {
  position: absolute;
  left: 32px;
  bottom: 23px;
  height: 2px;
  width: 200px;
  background: #e18718;
  content: "";
  display: inline-block;
  margin-left: 20px;
}
.separator:before {
  position: absolute;
  right: 32px;
  bottom: 23px;
  height: 2px;
  width: 200px;
  background: #e18718;
  content: "";
  display: inline-block;
  margin-right: 20px;
}
.separator2 {
  height: 4px;
  width:100%;
  margin: 10px 0px;
  background: #eeeeee;
  position: relative;
}

.separator3 {
  height: 2px;
  width: 75%;
  margin: 10px auto;
  background: #57936d;
}
.separator4 {
  height: 2px;
  width: 30px;
  margin:20px 0px;
  background: #f38181;
}

.separator5 {
  height: 2px;
  width: 75%;
