.scrooldown{
  position: absolute;
  left: calc(50% - 35px);
  bottom: 35px;
  width: 70px;
  height: 70px;
  line-height: 48px;
  background: none;
  border-radius: 50%;
  text-align: center;
  z-index: 9;
  animation: 1s latido infinite;
}
.scrooldown a i{
  font-size: 100px;
  line-height: 70px;
  color:white;
}

@keyframes latido{
  0%{transform:scale(1)}
  50%{transform:scale(1.1)}
  100%{transform:scale(1)}
}

